{
  "program": "examples/temp_monitor.wast",
  "out": "out/",
  "proxy": ["$reqTemp", "$isConnected"],
  "devices": [
    {
      "name": "remote VM",
      "host": "192.168.0.190",
      "port": 8080,
      "policy": "single-stop",
      "serial": "/dev/ttyUSB0",
      "enable": true,
      "is_remote": true
    },
    {
      "name": "local VM",
      "port": 8167,
      "enable": true
    }
  ]
}
